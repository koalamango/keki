{
  "name": "keki",
  "description": "KÄ“ki Artisan Cakes: gatsby+contentful+snipcart starter",
  "author": "Jessie Wang",
  "version": "0.0.1",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "ssr": "npm run build && npm run serve",
    "format": "prettier --config ./.prettierrc --write src/**/*.js",
    "format-css": "prettier --config ./.prettierrc --write src/**/*.css.js",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "npm run build:dir && npm run build:favicons && gatsby build",
    "test": "ava **/*.test.js --verbose",
    "eslint-fix": "eslint --fix src/**/*.js"
  },
  "greenkeeper": {
    "ignore": [
      "ava",
      "core-js"
    ]
  },
  "dependencies": {
