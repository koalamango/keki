    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "favicons": "^5.3.0",
    "gatsby-plugin-eslint": "^2.0.3",
    "gatsby-plugin-mdx": "^1.0.46",
    "lighthouse": "^4.0.0",
    "mkdirp": "^0.5.1",
    "node-gyp": "^3.8.0",
    "prettier": "^1.16.0"
